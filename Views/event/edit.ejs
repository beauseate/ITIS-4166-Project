<%- include('../partials/header.ejs')%>    
      <div class="leftContainer"></div>
      <div class="middleContainer">
        <form method = "POST" action = "/events/<%=event.id%>?_method=PUT">
            <h3 class="topicTitle">Category</h3>
            <select id="category" name="category">
              <option value="professional">Professional</option>
              <option value="social">Social</option>
            </select>
            <h3 class="titleTitle">Title</h3>
            <input type="text" id="title" name="title" value = "<%= event.title%>">
            <h3 class="hostTitle">Host</h3>
            <input type="text" id="host" name="host" value = "<%= event.host%>">
            <h3 class="detailsTitle">Details</h3>
            <input type="text" id="details" name="details" value = "<%= event.details%>">
            <h3 class="locationTitle">Location</h3>
            <input type="text" id="location" name="location" value = "<%= event.location%>">
            <h3 class="dateTitle">Date</h3>
            <input type="date" id="date" name="date" value = "<%= event.date%>">
            <h3 class="startTitle">Start</h3>
            <input type="time" id="startTime" name="startTime" value = "<%= event.startTiume%>">
            <h3 class="endTitle">End</h3>
            <input type="time" id="endTime" name="endTime" value = "<%= event.endTime%>">
            <h3 class="imageTitle">Image URL</h3>
            <input type="text" id="image" name="image" value = "<%= event.image%>">
            <input class="submitEventForm" type="submit" value="Submit">
          </form>
      </div>  
      <div class="rightContainer"></div>
<%- include('../partials/footer.ejs')%>